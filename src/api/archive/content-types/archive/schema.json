{
  "kind": "collectionType",
  "collectionName": "archives",
  "info": {
    "singularName": "archive",
    "pluralName": "archives",
    "displayName": "아카이브",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "startDate": {
      "type": "date"
    },
    "endDate": {
      "type": "date"
    },
    "thumbnail": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "description": {
      "type": "text"
    },
    "tags": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::archive-tag.archive-tag"
    },
    "units": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::archive-unit.archive-unit",
      "mappedBy": "exhibition"
    },
    "location": {
      "type": "string"
    },
    "host": {
      "type": "string"
    }
  }
}
